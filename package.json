{
  "name": "horus",
  "version": "0.1.0",
  "author": "Juan Luis Flores, Felipe, Adrian, Isreael Trujillo",
  "description": "Horus monitoring system",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^3.3.2",
    "@material-ui/icons": "^3.0.1",
    "@material-ui/lab": "^3.0.0-alpha.22",
    "ajv": "^6.5.5",
    "bootstrap": "^4.1.3",
    "cross-env": "^5.2.0",
    "date-fns": "^2.0.0-alpha.16",
    "electron-is-dev": "^1.0.1",
    "email-validator": "^2.0.4",
    "font-awesome": "^4.7.0",
    "jquery": "^3.3.1",
    "jsonlines": "^0.1.1",
    "material-ui-pickers": "^1.0.1",
    "moment": "^2.22.2",
    "nedb": "^1.8.0",
    "nodemailer": "^4.6.8",
    "numeral": "^2.0.6",
    "popper.js": "^1.14.4",
    "react": "^16.6.0",
    "react-dom": "^16.5.2",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.1",
    "recharts": "^1.2.0",
    "redux": "^4.0.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-json": "^1.0.0",
    "chai-json-pattern": "^1.1.0",
    "concurrently": "^4.0.1",
    "electron": "^3.0.0",
    "electron-builder": "^20.28.4",
    "electron-devtools-installer": "^2.2.4",
    "eslint-config-standard-jsx": "^6.0.2",
    "eslint-plugin-prettier": "^3.0.0",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-react-app": "^3.0.0",
    "mocha": "^5.2.0",
    "prettier": "^1.15.2",
    "spectron": "^5.0.0",
    "wait-on": "^3.0.1"
  },
  "build": {
    "appId": "com.horus",
    "productName": "Horus",
    "win": {},
    "mac": {},
    "linux": {}
  },
  "main": "electron/electron.js",
  "homepage": "./",
  "scripts": {
    "react-start": "react-scripts start",
    "react-build": "react-scripts build",
    "react-test": "react-scripts test --env=jsdom",
    "react-eject": "react-scripts eject",
    "electron-build": "electron-builder",
    "release": "npm run react-build && electron-builder --publish=always",
    "build": "npm run react-build && npm run electron-build",
    "start": "export DATA_ENV='LOCAL' && concurrently \"cross-env BROWSER=none npm run react-start\" \"wait-on http://localhost:3000 && electron .\"",
    "test": "export DATA_ENV='TEST' && cp test-json.jsonl test-jsoncopy.jsonl && mocha ./test/tests.js",
    "posttest": "rm -rf ./electron/datastore/test && cp test-jsoncopy.jsonl test-json.jsonl && rm test-jsoncopy.jsonl"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}